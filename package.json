    "axios": "^1.6.8",
    "formik": "^2.4.5",
    "next": "14.2.1",
    "react": "^18",
    "react-dom": "^18",
    "yarn": "^1.22.22",
    "yup": "^1.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
